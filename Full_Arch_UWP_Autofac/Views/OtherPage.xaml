<Page
    x:Class="Full_Arch_UWP_Autofac.Views.OtherPage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:Full_Arch_UWP_Autofac.Views"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    Background="{ThemeResource ApplicationPageBackgroundThemeBrush}">

    <Grid>
        <StackPanel>
            <TextBlock Text="OtherPage"
                       />
            <Button x:Name="ButtonDoPopup"
                    Content="DoPopup"
                    Command="{Binding OtherButtonClickCommand}"
                    CommandParameter="{Binding ElementName=ButtonDoPopup}"
                    Margin="10,10,0,0">
            </Button>
        </StackPanel>

    </Grid>
</Page>
